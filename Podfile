# Uncomment this line to define a global platform for your project
platform :ios, '8.0'
# Uncomment this line if you're using Swift
# use_frameworks!

target 'APPProject' do

#Json建立模型
pod 'JSONModel', '~> 1.2.0'

#网络请求框架,可以选择3.0之后的版本
pod 'AFNetworking', '~> 3.1.0'

#获取网络图片，同时图片轮播器控件需要用到
pod 'SDWebImage', '~> 3.7.6'

#OC代码加约束
pod 'Masonry', '~> 1.0.1'

#提示信息
pod 'Toast', '~> 3.0'

end

